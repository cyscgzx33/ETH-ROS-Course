<?xml version = "1.0"?>

<launch>

	<include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
		<arg name="world_name" default="/usr/share/gazebo-7/worlds/robocup14_spl_field.world"/>
		<arg name="laser_enabled" value="true"/>
	</include>

	<node name="teleop_twist_keyboard" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen"/>
	<node pkg="rviz" type="rviz" name="rviz"/>

	<node pkg="husky_highlevel_controller_ex_3" type="husky_highlevel_controller_ex_3" name="husky_highlevel_controller_ex_3"
	  output="screen" launch-prefix="gnome-terminal --command">
	  <rosparam command="load" file="$(find husky_highlevel_controller_ex_3)/config/config.yaml" />

	</node>



</launch>